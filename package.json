{
  "name": "rx-couch",
  "version": "1.2.0",
  "description": "RxJS-flavored APIs for CouchDB",
  "main": "index.js",
  "scripts": {
    "files": "find . -name '*.js' ! -path './node_modules/*' ! -path './coverage/*'",
    "test": "jshint `npm run -s files` && lintspaces -i js-comments -e .editorconfig `npm run -s files` && mocha",
    "test-travis": "jshint `npm run -s files` && lintspaces -i js-comments -e .editorconfig `npm run -s files` && ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangledfruit/rx-couch.git"
  },
  "keywords": [
    "couchdb",
    "rx",
    "rxjs",
    "nosql"
  ],
  "author": "tangledfruit <code@tangledfruit.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tangledfruit/rx-couch/issues"
  },
  "homepage": "https://github.com/tangledfruit/rx-couch#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "co-mocha": "^1.1.2",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.1",
    "jshint": "^2.9.1-rc2",
    "lintspaces-cli": "^0.1.1",
    "mocha": "^2.3.4",
    "nock": "^3.6.0",
    "rx-to-async-iterator": "^1.0.1"
  },
  "dependencies": {
    "deep-eql": "^0.1.3",
    "deepmerge": "^0.2.10",
    "rx": "^4.0.7",
    "rx-fetch": "^1.1.1",
    "shallow-copy": "0.0.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
